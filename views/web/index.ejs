				</div>
			</div>
		</div>
	</div>
	</div>
<!-- about -->
<!-- services -->
	<div class="services">
		<div class="container">
			<a href="/blog" style="text-decoration: none"><h3>个人博客</h3></a>
			<div class="container">
				<ul class="timeline">
					<% articles.forEach(function (article,index) { %>
						<% if (index%2 == 0){%>
							<li>
								<div class="timeline-badge info"><i class="fa fa-file-text"></i></div>
								<div class="timeline-panel">
									<div class="timeline-heading">
										<h4 class="timeline-title"><%=article.title%></h4>
										<span>发布时间：<%=article.date.minute%></span>
										<span>作者：<%=article.author%></span>
										<span>标签：<%=article.tag%></span>
										<hr />
									</div>
									<div class="timeline-body">
										<p><%- article.content%>
										</p>
									</div>
								</div>
							</li>
						<%}else {%>
								<li class="timeline-inverted">
									<div class="timeline-badge info"><i class="fa fa-file-text" style="color: #000"></i></div>
									<div class="timeline-panel">
										<div class="timeline-heading">
											<h4 class="timeline-title"><%=article.title%></h4>
											<span>发布时间：<%=article.date.minute%></span>
											<span>作者：<%=article.author%></span>
											<span>标签：<%=article.tag%></span>
											<hr />
										</div>
										<div class="timeline-body">
											<p><%- article.content%>
											</p>
										</div>
									</div>
								</li>
						<%}%>
					<% }) %>

				</ul>
				<a href="/blog" class="btn btn-primary col-sm-offset-5 col-sm-2" style="margin-bottom: 20px;"> 查看更多</a>
			</div>
	</div>
<!-- services -->
<!-- portfolio -->

	<a href="#" id="toTop" style="display: block;"><span id="toTopHover" style="opacity: 0;"></span></a>
